{
  "name": "OAuth Tutorial FriendBlock",
  "version": "1.0",
  "description": "Uses OAuth to connect to Google's People API and display contacts photos.",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "identity",
    "storage"
  ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "oauth2": {
    "client_id": "672737831445-3lpk9h0h0hl104oubfafgm4jvov0lno5.apps.googleusercontent.com",
    "scopes":[
      "https://www.googleapis.com/auth/youtube"]
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://www.youtube.com/ https://s.ytimg.com; object-src 'self'",
  "key": ""
}
